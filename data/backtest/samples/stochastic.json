{
  "methods": [
    {
      "method": "STOCH",
      "name": "stoch-k",
      "kPeriod": 14,
      "kSmooth": 3,
      "dSmooth": 3,
      "output": "k"
    },
    {
      "method": "STOCH",
      "name": "stoch-d",
      "extends": "stoch-k",
      "output": "d"
    },
    {
      "method": "VALUE",
      "name": "overbought",
      "value": 80
    },
    {
      "method": "VALUE",
      "name": "oversold",
      "value": 20
    }
  ],
  "risk": {
    "method": "PERCENTAGE",
    "percentage": 10
  },
  "positions": [
    {
      "direction": "long",
      "entry": {
        "repeat": "sequence",
        "signal": {
          "filter": "ALL_OF",
          "conditions": [
            {
              "filter": "CROSSOVER",
              "signal": "stoch-k",
              "reference": "stoch-d"
            },
            {
              "filter": "LESS_THAN",
              "target": "stoch-k",
              "threshold": "oversold"
            }
          ]
        }
      },
      "exit": {
        "signal": {
          "filter": "ALL_OF",
          "conditions": [
            {
              "filter": "CROSSUNDER",
              "signal": "stoch-k",
              "reference": "stoch-d"
            },
            {
              "filter": "GREATER_THAN",
              "target": "stoch-k",
              "threshold": "overbought"
            }
          ]
        }
      },
      "stopLoss": true,
      "takeProfit": {
        "riskMultiplier": 2
      }
    }
  ]
}