{
  "version": 1,
  "series": {
    "stoch": {
      "method": "STOCH",
      "kPeriod": 14,
      "kSmooth": 3,
      "dSmooth": 3
    },
    "stoch_k": {
      "method": "SELECT_OUTPUT",
      "name": "k-percent",
      "source": {
        "method": "REFERENCE",
        "name": "stoch"
      }
    },
    "stoch_d": {
      "method": "SELECT_OUTPUT",
      "name": "d-percent",
      "source": {
        "method": "REFERENCE",
        "name": "stoch"
      }
    },
    "overbought": {
      "method": "VALUE",
      "value": 80
    },
    "oversold": {
      "method": "VALUE",
      "value": 20
    }
  },
  "risk": {
    "method": "PERCENTAGE",
    "percentage": 10
  },
  "stopLoss": true,
  "takeProfit": {
    "riskMultiplier": 2
  },
  "longPosition": {
    "entry": {
      "signal": {
        "filter": "ALL_OF",
        "conditions": [
          {
            "filter": "CROSSOVER",
            "signal": {
              "method": "REFERENCE",
              "name": "stoch_k"
            },
            "reference": {
              "method": "REFERENCE",
              "name": "stoch_d"
            }
          },
          {
            "filter": "LESS_THAN",
            "target": {
              "method": "REFERENCE",
              "name": "stoch_k"
            },
            "threshold": {
              "method": "REFERENCE",
              "name": "oversold"
            }
          }
        ]
      }
    },
    "exit": {
      "signal": {
        "filter": "ALL_OF",
        "conditions": [
          {
            "filter": "CROSSUNDER",
            "signal": {
              "method": "REFERENCE",
              "name": "stoch_k"
            },
            "reference": {
              "method": "REFERENCE",
              "name": "stoch_d"
            }
          },
          {
            "filter": "GREATER_THAN",
            "target": {
              "method": "REFERENCE",
              "name": "stoch_k"
            },
            "threshold": {
              "method": "REFERENCE",
              "name": "overbought"
            }
          }
        ]
      }
    }
  }
}