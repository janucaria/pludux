{
  "methods": [
    {
      "method": "STOCH_RSI",
      "name": "stoch_rsi-k",
      "kPeriod": 14,
      "kSmooth": 3,
      "dSmooth": 3,
      "output": "k",
      "rsiInput": "CLOSE",
      "rsiPeriod": 14
    },
    {
      "method": "STOCH_RSI",
      "name": "stoch_rsi-d",
      "extends": "stoch_rsi-k",
      "output": "d"
    },
    {
      "method": "VALUE",
      "name": "overbought",
      "value": 80
    },
    {
      "method": "VALUE",
      "name": "oversold",
      "value": 20
    }
  ],
  "risk": {
    "method": "ATR",
    "atr": {
      "period": 14,
      "multiplier": 2
    },
    "percentage": 5,
    "value": 1000
  },
  "stopLoss": true,
  "takeProfit": {
    "riskMultiplier": 3
  },
  "longPosition": {
    "entry": {
      "signal": {
        "filter": "ALL_OF",
        "conditions": [
          {
            "filter": "CROSSOVER",
            "signal": "stoch_rsi-k",
            "reference": "stoch_rsi-d"
          },
          {
            "filter": "LESS_THAN",
            "target": "stoch_rsi-k",
            "threshold": "oversold"
          }
        ]
      }
    },
    "exit": {
      "signal": {
        "filter": "ALL_OF",
        "conditions": [
          {
            "filter": "CROSSUNDER",
            "signal": "stoch_rsi-k",
            "reference": "stoch_rsi-d"
          },
          {
            "filter": "GREATER_THAN",
            "target": "stoch_rsi-k",
            "threshold": "overbought"
          }
        ]
      }
    }
  }
}