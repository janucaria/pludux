{
  "series": {
    "stoch_rsi": {
      "method": "STOCH_RSI",
      "kPeriod": 14,
      "kSmooth": 3,
      "dSmooth": 3,
      "rsiInput": "CLOSE",
      "rsiPeriod": 14
    },
    "stoch_rsi_k": {
      "method": "OUTPUT_BY_NAME",
      "name": "percent-k",
      "source": {
        "method": "REFERENCE",
        "name": "stoch_rsi"
      }
    },
    "stoch_rsi_d": {
      "method": "OUTPUT_BY_NAME",
      "name": "percent-d",
      "source": {
        "method": "REFERENCE",
        "name": "stoch_rsi"
      }
    },
    "overbought": {
      "method": "VALUE",
      "value": 80
    },
    "oversold": {
      "method": "VALUE",
      "value": 20
    }
  },
  "risk": {
    "method": "ATR",
    "atr": {
      "period": 14,
      "multiplier": 2
    },
    "percentage": 5,
    "value": 1000
  },
  "stopLoss": true,
  "takeProfit": {
    "riskMultiplier": 3
  },
  "longPosition": {
    "entry": {
      "signal": {
        "filter": "ALL_OF",
        "conditions": [
          {
            "filter": "CROSSOVER",
            "signal": {
              "method": "REFERENCE",
              "name": "stoch_rsi_k"
            },
            "reference": {
              "method": "REFERENCE",
              "name": "stoch_rsi_d"
            }
          },
          {
            "filter": "LESS_THAN",
            "target": {
              "method": "REFERENCE",
              "name": "stoch_rsi_k"
            },
            "threshold": {
              "method": "REFERENCE",
              "name": "oversold"
            }
          }
        ]
      }
    },
    "exit": {
      "signal": {
        "filter": "ALL_OF",
        "conditions": [
          {
            "filter": "CROSSUNDER",
            "signal": {
              "method": "REFERENCE",
              "name": "stoch_rsi_k"
            },
            "reference": {
              "method": "REFERENCE",
              "name": "stoch_rsi_d"
            }
          },
          {
            "filter": "GREATER_THAN",
            "target": {
              "method": "REFERENCE",
              "name": "stoch_rsi_k"
            },
            "threshold": {
              "method": "REFERENCE",
              "name": "overbought"
            }
          }
        ]
      }
    }
  }
}