{
  "version": 6,
  "configurePresets": [
    {
      "name": "emscripten-debug-user",
      "inherits": "emscripten-debug",
      "installDir": "${sourceDir}/.out/webroot"
    },
    {
      "name": "emscripten-release-user",
      "inherits": "emscripten-release",
      "installDir": "${sourceDir}/.out/webroot"
    }
  ],
  "packagePresets": [
    {
      "name": "msvc-pack-release",
      "configurePreset": "msvc-release",
      "packageDirectory": "${sourceDir}/.out/packages",
      "generators": [
        "ZIP"
      ]
    }
  ]
}